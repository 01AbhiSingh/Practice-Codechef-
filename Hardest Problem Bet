#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    int x,y,z;
	    cin>>x>>y>>z;
	    if((x<y)&&(x<z)){
	        cout<<"draw"<<endl;
	    }
	    else if((x>y)&&(z>y)){
	        cout<< "bob"<<endl;
	    }
	   else{
	       cout<<"alice"<< endl;
	   }
	}
	return 0;
}
