#include <iostream>
using namespace std;

int main(){
    int t;
    cin>>t;
    while(t--){
        
       int n,p,x,y,i;
       cin>>n>>p>>x>>y;
       int arr[n];
       for(int i=0;i<n;i++){
           cin>>arr[i];
       }
       int totalTime=0;
       for(int i=0;i<p;i++){
           if(arr[i]==0)
                totalTime=totalTime+x;
            else
                totalTime=totalTime+y;
       }
       cout<<totalTime<<endl;
       
       
    }
    return 0;
}
