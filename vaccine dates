#include <iostream>

using namespace std;

int main()
{
    int t;
    cin >> t;
    while (t--)
    {
        int x,y,z;
        cin>>x>>y>>z;
            if (x < y)
        {
            cout << "Too Early\n";
        }
        else if (x >z)
        {
            cout << "Too Late\n";
        }
        else if (x >= y && z >= x)
        {
            cout << "Take second dose now\n";
        }
    }
}
