#include <iostream>
using namespace std;

int main() {
	int t,a,b,a1,b1,a2,b2;
	cin>>t;
	while(t-->0){
	    cin>>a>>b>>a1>>b1>>a2>>b2;
	    int ans=0;
	    if((a1==a && b1==b) or (a1==b && b1==a)){
	        ans=1;
	       
	    }
	    if((a2==a && b2==b) or (a2==b && b2==a)){
	        ans=2;
	       
	    }
	    cout<<ans<<endl;
	}
	return 0;
}
