#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    bool a,b,c;
	    cin>>a>>b>>c;
	    if(a==0&&(b==1||c==1)){
	        cout<<true<<endl;
	    }
	    else if(a==1&&(b==0||c==0)){
	        cout<<true<<endl;
	    }
	    else{
	        cout<<false<<endl;
	    }
	}
	return 0;
}
