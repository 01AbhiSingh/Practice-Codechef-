#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int a,b,c,n1,n2,n3;
	    cin>>a>>b>>c;
	    n1=a+b;
	    n2=b+c;
	    n3=a+c;
	    
	    if(n1>n3){
	        if(n1>n2){
	            cout<<n1<<endl;
	        }
	        else{
	            cout<<n2<<endl;
	        }
	    }
	    else if(n3>n2){
	        cout<<n3<<endl;
	    }
	    else{
	        cout<<n2<<endl;
	    }
	}
	return 0;
}
